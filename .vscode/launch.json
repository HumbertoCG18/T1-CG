{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch MSYS2 UCRT64",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/BasicoOpenGL.exe",
      "cwd": "${workspaceFolder}",
      "MIMode": "gdb",
      "miDebuggerPath": "C:/msys64/ucrt64/bin/gdb.exe",
      "externalConsole": true,
      "stopAtEntry": false
    }
  ]
}
